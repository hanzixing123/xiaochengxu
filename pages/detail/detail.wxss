/*主容器*/
.stepper {
	width: 100px;
	height: 26px;
	/*给主容器设一个边框*/
	border: 1px solid #ccc;
  border-radius: 3px;
  margin-left: 30px;
	/* margin:0 auto; */
}
 
/*加号和减号*/
.stepper text {
	width: 29px;
	line-height: 26px;
	text-align: center;
  float: left;
  margin: 0 auto;

}
 
/*数值*/
.stepper input {
	width: 40px;
	height: 26px;
	float: left;
	margin: 0 auto;
	text-align: center;
	font-size: 12px;
	/*给中间的input设置左右边框即可*/
	border-left: 1px solid #ccc;
	border-right: 1px solid #ccc;
}
 
/*普通样式*/
.stepper .normal{
	color: black;
}
 
/*禁用样式*/
.stepper .disabled{
	color: #ccc;
}
.goods_num{
  margin-left: 30px;
  font-size: 15px;
}
